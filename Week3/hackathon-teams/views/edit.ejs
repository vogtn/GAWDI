<div>
  <h2>Edit <%= team.name %></h2>
  <form action='/teams/<%= team.name %>' class='put-form'>
    <div class="form-group">
      <label for="teamName">Team Name</label>
      <input type="text" name="name" class="form-control" id="teamName" value="<% team.name %>">
    </div>
    <div class="form-group">
      <label for="teamMembers">Team Members</label>
      <input type="text" name="members" class="form-control" id="teamMembers" value="<% team.members %>">
    </div>
    <input type="submit" class="btn btn-primary">
</div>
